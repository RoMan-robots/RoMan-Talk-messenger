{
    "name": "roman_talk",
    "version": "2.0.0",
    "description": "My messenger, called RoMan Talk, here is code from it",
    "main": "electron.js",
    "type": "module",
    "scripts": {
        "start-electron": "electron .",
        "package-mac": "electron-packager . RoManTalk --platform=darwin --arch=arm64 --out=dist --overwrite --icon=../backend/images/favicon.icns",
        "package-win": "electron-packager . RoManTalk --platform=win32 --arch=ia32,x64 --out=dist --overwrite --icon=../backend/images/favicon.ico"
    },
    "keywords": [
        "RoMan",
        "Talk",
        "RoMan Talk",
        "Messenger"
    ],
    "author": "RoMan",
    "license": "ISC",
    "devDependencies": {
        "electron": "^32.0.1",
        "electron-packager": "^17.1.2"
    }
}
